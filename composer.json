{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "symfony/console": "4.3.*",
    "symfony/dotenv": "4.3.*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "4.3.*",
    "symfony/orm-pack": "^1.0",
    "symfony/yaml": "4.3.*"
  },
  "require-dev": {
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "phpstan/phpstan": "^0.11.8",
    "phpunit/phpunit": "^8.1",
    "symfony/maker-bundle": "^1.11",
    "symfony/var-dumper": "4.3.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "test:unit": "vendor/bin/phpunit --testdox --color=always",
    "test:stan:src": "vendor/bin/phpstan analyse src --level=max",
    "tests": [
      "@test:unit",
      "@test:stan:src"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.3.*"
    }
  }
}
